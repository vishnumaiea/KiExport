{
  "name": "KiExport.JSON",
  "description": "Configuration file for KiExport",
  "filetype": "json",
  "version": "1.9",
  "project_name": "",
  "commands": [
    "pcb_drc",
    "gerbers",
    "sch_erc",
    "sch_pdf",
    ["bom", "CSV"],
    ["bom", "XLS"],
    ["bom", "HTML"],
    "pcb_pdf",
    "pcb_fab_pdf",
    "positions",
    "svg",
    ["pcb_render", "PCB-Front"],
    ["pcb_render", "PCB-Back"],
    ["pcb_render", "PCB-Left"],
    ["pcb_render", "PCB-Right"],
    ["pcb_render", "PCB-Parts-Front"],
    ["pcb_render", "PCB-Parts-Back"],
