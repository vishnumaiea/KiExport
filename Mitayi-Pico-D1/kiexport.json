      "--format": "report",
      "--all-track-errors": false,
      "--schematic-parity": true,
      "--units": "mm",
      "--severity-all": false,
      "--severity-error": true,
      "--severity-warning": true,
      "--severity-exclusions": false,
      "--exit-code-violations": false
    },
    "gerbers": {
      "--output_dir": "Output",
      "--layers": [
        "F.Cu",
        "B.Cu",
        "F.Silkscreen",
        "B.Silkscreen",
        "F.Mask",
        "B.Mask",
        "User.Drawings",
        "Edge.Cuts"
